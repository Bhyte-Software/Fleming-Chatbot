<Siml>
  <BotSettings>
    <!--"Name" is the name the bot refers itself with.-->
    <Variable Name="BotName" Value="Fleming" />
    <Variable Name="EvaluateButtonText" Value="Evaluate" />
    <!--Response that is added if No internal response is generated-->
    <Variable Name="Empty-Response" Value="Sorry, no response was generated." />
    <!--Determines the maximum time limit in milliseconds for processing a response-->
    <!--Note: For Syn Web Assistant this value is automatically overridden and is set to 4000 milliseconds-->
    <Variable Name="Timeout" Value="5000" />
    <!--Used to Separate variables in listed formats example 1, 2, 3-->
    <Variable Name="List-Separator" Value=", " />
    <!--Adds a delimiter when a List is requested example 1, 2, 3 AND 4-->
    <Variable Name="List-End" Value=" and " />
    <!--Culture: Is used to let SIML Bot know what environment culture is to be used.-->
    <Variable Name="CULTURE" Value="en-UK" />
    <!--Timeout-Response: Response that is generated when the chat request gets timed out-->
    <Variable Name="Timeout-Response" Value="Sorry, Your chat request has timed out." />
    <!--Emotion: Default bot emotion at startup. Refer EmotionML-->
    <Variable Name="Emotion" Value="pleased" />
    <!--Undefined: Value that renders a variable 'Undefined'-->
    <Variable Name="Undefined" Value="" />
    <!--Default: Value that is returned if a Get function is used for an undefined variable-->
